        <content type="html"><![CDATA[<p>测试，测试ing<br>#include&lt;bits&#x2F;stdc++.h&gt;<br>using namespace std;<br>typedef long long ll;<br>const int N&#x3D;205;<br>ll n,m,a[N][N];<br>ll dx[8]&#x3D;{0,-1,0,1,0},dy[8]&#x3D;{0,0,1,0,-1};<br>void dfs(ll x,ll y){<br>    if(x&lt;0||y&lt;0||x&gt;n+1||y&gt;n+1||a[x][y]&#x3D;&#x3D;1||a[x][y]&#x3D;&#x3D;2) return;<br>    a[x][y]&#x3D;2;<br>    for(int i&#x3D;1;i&lt;&#x3D;4;i++) dfs(x+dx[i],y+dy[i]);<br>}<br>int main(){<br>    scanf(“%lld”,&amp;n);<br>    for(int i&#x3D;1;i&lt;&#x3D;n;i++)<br>        for(int j&#x3D;1;j&lt;&#x3D;n;j++)<br>            scanf(“%lld”,&amp;a[i][j]);<br>    dfs(0,0);<br>    for(int i&#x3D;1;i&lt;&#x3D;n;i++){<br>        for(int j&#x3D;1;j&lt;&#x3D;n;j++){<br>            if(a[i][j]&#x3D;&#x3D;2) printf(“0 “);<br>            if(a[i][j]&#x3D;&#x3D;1) printf(“1 “);<br>            if(a[i][j]&#x3D;&#x3D;0) printf(“2 “);<br>        }<br>        printf(“\n”);<br>    }<br>}</p>]]></content>
      
      
